from cryptography.hazmat.primitives.ciphers.aead import AESGCM

# Values from your example (convert from hex to bytes)
aesKey = bytes.fromhex(str(input("Enter in your AES Key: ")))
nonce = bytes.fromhex(str(input("Enter in your nonce: ")))
cipherText = bytes.fromhex(str(input("Enter in your ciphertext: ")))

# Create AES-GCM with your key
aesGcm = AESGCM(aesKey)

# Decrypt
plainText = aesGcm.decrypt(nonce, cipherText, None)

print("Decrypted message:", plainText.decode())